{
  "name": "puppet-openvpn",
  "version": "8.2.1-rc0",
  "author": "Vox Pupuli",
  "summary": "OpenVPN server puppet module",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-openvpn",
  "project_page": "https://github.com/voxpupuli/puppet-openvpn",
  "issues_url": "https://github.com/voxpupuli/puppet-openvpn/issues",
  "description": "Puppet module to manage OpenVPN servers",
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    },
    {
      "operatingsystem": "FreeBSD"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.8 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 4.1.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.0 < 7.0.0"
    }
  ],
  "tags": [
    "vpn",
    "openvpn"
  ]
}
